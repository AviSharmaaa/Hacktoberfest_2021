<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/back3"
    android:layout_height="match_parent"
    tools:context="com.example.laksh_pc.voteapp.votes">

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="26dp"
        android:text="Vote Status:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView16"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="22dp"
        android:text="President"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView17"
        android:layout_below="@+id/imageView13"
        android:layout_marginTop="59dp"
        android:text="Governor"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/black" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/pres1"
        android:layout_alignTop="@+id/imageView15"
        android:layout_alignParentStart="true"
        android:layout_marginStart="66dp" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="71dp"
        android:layout_marginTop="16dp"
        app:srcCompat="@drawable/pres2"
        android:layout_below="@+id/textView17"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="90dp"
        android:layout_height="90dp"
        app:srcCompat="@drawable/gov2"
        android:layout_below="@+id/textView18"
        android:layout_alignStart="@+id/imageView13" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="90dp"
        android:layout_height="90dp"
        app:srcCompat="@drawable/gov3"
        android:layout_below="@+id/textView18"
        android:layout_alignEnd="@+id/imageView15" />

    <TextView
        android:id="@+id/viewcount"
        android:layout_width="50dp"
        android:gravity="center"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/imageView16"
        android:layout_below="@+id/imageView13"
        android:layout_marginEnd="18dp"
        android:layout_marginTop="12dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/viewcount1"
        android:layout_width="50dp"
        android:gravity="center"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/imageView17"
        android:layout_alignTop="@+id/viewcount"
        android:layout_marginStart="13dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/viewcount2"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:gravity="center"

        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/black"
        android:layout_below="@+id/imageView16"
        android:layout_alignStart="@+id/viewcount" />

    <TextView
        android:id="@+id/viewcount3"
        android:layout_width="50dp"
        android:gravity="center"
        android:layout_height="40dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/black"
        android:layout_below="@+id/imageView17"
        android:layout_alignStart="@+id/viewcount1" />

</android.widget.RelativeLayout>
